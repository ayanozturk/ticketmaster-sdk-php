{
  "name": "ayanozturk/ticketmaster-sdk-php",
  "license": "MIT",
  "description": "TicketMaster API SDK",
  "require": {
    "php": "^7.4.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "guzzlehttp/guzzle": "^7.0",
    "doctrine/common": "^3.0",
    "symfony/serializer": "^5.1",
    "symfony/property-access": "^5.1",
    "jms/serializer": "^3.8"
  },

  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5"
  },

  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
